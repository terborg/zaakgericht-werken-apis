{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Docusaurus",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/node_modules/.bin/docusaurus",
        "args": ["start"],
        "cwd": "${workspaceFolder}",
        "port": 3000
      }
    ]
  }
  